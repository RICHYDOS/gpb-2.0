{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "biginteger",
      "required": true
    },
    "discountPrice": {
      "type": "biginteger"
    },
    "productImage": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": true
    }
  }
}
